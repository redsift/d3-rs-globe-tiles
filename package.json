{
  "name": "t3-rs-geo-tiles",
  "version": "0.0.2",
  "description": "Tile generator for t3-rs-globe",
  "main": "src/Grid.js",
  "bin": {
    "buildgrid": "bin/buildgrid"
  },
  "scripts": {
    "build": "mkdir -p tiles && bin/buildgrid -t -d 60 -r 500 -w 0.9 -c 0.55 -o ./tiles/grid.js -v resources/population_density_1994.json resources/equirectangle_projection.png",
    "pretest": "npm outdated || true && npm run build",
    "test": "tape 'test/**/*-test.js'",
    "posttest": "eslint index.js src",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redsift/t3-rs-geo-tiles.git"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": {
    "name": "Rahul Powar",
    "url": "https://medium.com/@rahulpowar"
  },
  "license": "MIT",
  "dependencies": {
    "d3-geo": "^1.4.1",
    "d3-geo-projection": "^1.2.1",
    "hexasphere.js": "*",
    "minimist": "*",
    "pngjs": "~3.0.0"
  },
  "devDependencies": {
    "tape": "*"
  }
}
